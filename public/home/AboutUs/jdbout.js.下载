$(function(){var a=$(".o-hd").attr("data-attr");$(".l-menu-list").find("#"+a).addClass("h-current");var b=$(".timeline-list .timeline-item");var c=b.length;b.first().addClass("timeline-on"),b.filter(":gt(13)").hide(),b.hover(function(){$(this).addClass("timeline-on").siblings().removeClass("timeline-on")}),$(".btn-timeline").each(function(){var a=$(this);13>c&&a.hide(),a.click(function(){b.filter(":gt(13)").slideDown(),a.fadeOut()})}),$(".b-thickbox-wp").each(function(){var a=$(this).find(".icon-thickbox-close,.btn-thickbox-sure");a.click(function(){var a=$(this).parents(".b-thickbox-wp");a.fadeOut(),$(".thickbox-mask").fadeOut(),setCookie("data-cookie","cookie-ys","31536000")})}),"cookie-ys"==getCookie("data-cookie")&&$(".b-thickbox-wp,.thickbox-mask").hide()});function getCookie(a){var b=a+"=";var c=document.cookie.split(";");for(var d=0;d<c.length;d++){var e=c[d];for(;" "==e.charAt(0);)e=e.substring(1,e.length);if(0==e.indexOf(b))return unescape(e.substring(b.length,e.length))}return!1}function clearCookie(a){setCookie(a,"",-1)}function setCookie(a,b,c){c=c||0;var d="";if(0!=c){var e=new Date;e.setTime(e.getTime()+1e3*c),d="; expires="+e.toGMTString()}document.cookie=a+"="+escape(b)+d+"; path=/"}